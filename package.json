{
  "name": "cryptobot",
  "version": "1.0.0",
  "description": "Crypto auction bot with MongoDB and Redis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
