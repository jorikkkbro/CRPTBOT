    "test:middle": "npx ts-node tests/middle.test.ts",
    "test:high": "npx ts-node tests/high.test.ts",
    "test:overhigh": "npx ts-node tests/overhigh.test.ts"
  },
  "dependencies": {
    "bullmq": "^5.1.0",
